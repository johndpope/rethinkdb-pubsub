{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "object-curly-spacing": ["warn", "never"],
    "indent": ["error", 4],
    "func-names": "off",
    "space-before-function-paren": ["error", "never"],
    "max-len": ["error", 120, 4],
    "no-param-reassign": 0,
    "no-underscore-dangle": 0,
    "no-return-assign": 0,
    "no-console": 0
  }
}
